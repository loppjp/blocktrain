    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "blocktrain init",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/env/bin/blocktrain",
            "console": "integratedTerminal",
            "args": [
                "init"
            ]
        },
        {
            "name": "Test All",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
        },
        {
            "name": "Test Instantiate Experiment",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": ["tests/test_instantiation.py::TestInstantiateExperiment"]
        },
        {
            "name": "Test Instantiate Dataset",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": ["tests/test_instantiation.py::TestInstantiateIRDataset"]
        },
        {
            "name": "Test Instantiate Model",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": ["tests/test_instantiation.py::TestInstantiateModel"]
        },
        {
            "name": "Test Instantiate Optimizer",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": ["tests/test_instantiation.py::TestInstantiateOptimizer"]
        }
    ]
}